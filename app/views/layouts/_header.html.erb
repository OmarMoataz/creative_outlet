<header class="navbar navbar-fixed-top navbar-inverse header">
  <div class="container">
    <div class="logo"> <%= link_to "Members Only", root_url, class: "btn" %> </div>
    <ul class="nav navbar-nav navbar-right">
      <% if logged_in? %>
        <li> <%= current_user.name %> </li>
        <li>  <%= link_to "Log out", logout_path, method: :delete, class: "btn" %> </li>
      <% else %>
      <li><%= link_to "Log in", login_path %></li>
      <% end %>
    </ul>
  </div>
</header>